<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane style="-fx-background-color: #749C75;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0">
         <children>
            <Label fx:id="loginLabel" text="Login Page" textFill="WHITE">
               <font>
                  <Font size="96.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="usernameLabel" text="Username:" textFill="WHITE" />
                  <TextField />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="passwordLabel" text="Password:" textFill="WHITE" />
                  <TextField />
               </children>
            </HBox>
            <Button fx:id="loginButton" mnemonicParsing="false" text="Login" />
            <Button fx:id="registerButton" mnemonicParsing="false" text="Register">
               <VBox.margin>
                  <Insets top="300.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</Pane>
